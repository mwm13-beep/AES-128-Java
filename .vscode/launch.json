    "version": "0.2.0",
    "configurations": [

        {
            "type": "java",
            "name": "Debug (Launch)-AESTest",
            "request": "launch",
            "mainClass": "projectone.AESTest",
            "projectName": "aes",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "AES",
            "request": "launch",
            "mainClass": "projectone.AES",
            "projectName": "aes"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Main",
            "request": "launch",
            "mainClass": "AES",
            "args": ["e", "key.txt", "plainText.txt"]
        }
    ]
}